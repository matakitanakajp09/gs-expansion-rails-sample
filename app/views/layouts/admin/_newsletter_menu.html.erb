<div class="l-side">
  <div class="sw-main-header">
    <div class="sw-main-header-title"><%= t ".title" %></div>
  </div>
  <div class="sw-main-body">
    <div class="user-links">
      <div class="sw-menu">
        <ul class="sw-menu-link">
          <li>
            <%= link_to admin_newsletters_path(tab: "all") do %>
              <span><%= t ".newsletter" %></span>
            <% end %>
          </li>
          <li>
            <%= link_to admin_newsletter_subscribers_path do %>
              <span><%= t ".newsletter_subscriber" %></span>
            <% end %>
          </li>
          <li>
            <%= link_to admin_deleted_newsletter_subscribers_path do %>
              <span><%= t ".deleted_newsletter_subscriber" %></span>
            <% end %>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>
